<Window x:Class="Taskalu.MemoWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Taskalu"
        mc:Ignorable="d"
        Title="Memo - taskalu" Height="320" Width="500" Loaded="MemoWindow_Loaded"  >
    <Grid>
        <StackPanel>
            <TextBox x:Name="DateTextBox"></TextBox>
            <TextBox x:Name="MemoTextBox" Height="270" ScrollViewer.VerticalScrollBarVisibility="Auto"></TextBox>
        </StackPanel>
    </Grid>
</Window>
